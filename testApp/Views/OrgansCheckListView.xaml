                </b:Interaction.Triggers>
            </CheckBox>
            <CheckBox Content="Liver"
                      Foreground="LawnGreen">
                <b:Interaction.Triggers>
                    <b:EventTrigger EventName="Checked">
                        <b:CallMethodAction MethodName="CheckBox_Checked"
                                            TargetObject="{Binding}" />
                    </b:EventTrigger>
                </b:Interaction.Triggers>
            </CheckBox>
            <CheckBox Content="Stomach"
                      Foreground="ForestGreen">
                <b:Interaction.Triggers>
                    <b:EventTrigger EventName="Checked">
                        <b:CallMethodAction MethodName="CheckBox_Checked"
                                            TargetObject="{Binding}" />
                    </b:EventTrigger>
                </b:Interaction.Triggers>
            </CheckBox>
        </StackPanel>
    </Grid>
</UserControl>
